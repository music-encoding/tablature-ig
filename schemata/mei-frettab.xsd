  <xs:element name="halfmRpt">
    <xs:annotation>
      <xs:documentation>(half-measure repeat) – A half-measure repeat in any meter.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
