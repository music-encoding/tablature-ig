    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="mei:dot"/>
        <xs:element ref="mei:rhythmGlyph"/>
        <xs:group ref="mei:model.appLike"/>
        <xs:group ref="mei:model.editLike"/>
        <xs:group ref="mei:model.transcriptionLike"/>
      </xs:choice>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.rest.log.attributes"/>
      <xs:attributeGroup ref="mei:att.rest.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.rest.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.rest.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="role">
    <xs:annotation>
      <xs:documentation>Name of a dramatic role, as given in a cast list.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mei:model.textPhraseLike.limited"/>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.lang.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="roleDesc">
    <xs:annotation>
      <xs:documentation>(role description) – Describes a character's role in a drama.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mei:model.textPhraseLike.limited"/>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.lang.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sb">
    <xs:annotation>
