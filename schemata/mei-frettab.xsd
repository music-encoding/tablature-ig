      <xs:choice>
        <xs:sequence>
          <xs:group ref="mei:model.staffLike"/>
          <xs:group maxOccurs="unbounded" ref="mei:model.staffLike"/>
        </xs:sequence>
        <xs:sequence>
          <xs:group ref="mei:model.layerLike"/>
          <xs:group maxOccurs="unbounded" ref="mei:model.layerLike"/>
