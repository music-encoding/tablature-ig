      <xs:attributeGroup ref="mei:att.gliss.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.gliss.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.gliss.anl.attributes"/>
      <xs:attributeGroup ref="mei:att.typed.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="hairpin">
