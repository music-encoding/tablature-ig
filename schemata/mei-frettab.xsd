        <xs:restriction base="xs:token">
          <xs:enumeration value="horizontal">
            <xs:annotation>
              <xs:documentation/>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="vertical">
            <xs:annotation>
              <xs:documentation/>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.mensur.vis.attribute.orient">
