    <xs:attributeGroup ref="mei:att.color.attributes"/>
    <xs:attributeGroup ref="mei:att.relativeSize.attributes"/>
    <xs:attributeGroup ref="mei:att.staffLoc.attributes"/>
    <xs:attributeGroup ref="mei:att.typography.attributes"/>
    <xs:attributeGroup ref="mei:att.mensur.vis.attribute.form"/>
    <xs:attributeGroup ref="mei:att.mensur.vis.attribute.orient"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.mensur.vis.attribute.form">
