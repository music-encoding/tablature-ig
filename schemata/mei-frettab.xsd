    <xs:attributeGroup ref="mei:att.beamSecondary.attribute.breaksec"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.beamSecondary.attribute.breaksec">
    <xs:attribute name="breaksec" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>Presence of this attribute indicates that the secondary beam should be broken following this note/chord. The value of the attribute records the number of beams which should remain unbroken.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
