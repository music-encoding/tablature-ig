    <xs:attribute name="beatDef">
      <xs:annotation>
        <xs:documentation>Indicates the performed duration represented by the beatRpt symbol.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union memberTypes="mei:data.DURATION.gestural">
          <xs:simpleType>
            <xs:restriction base="xs:token">
