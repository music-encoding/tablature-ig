      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.bibl.attributes"/>
      <xs:attributeGroup ref="mei:att.lang.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="useRestrict">
    <xs:annotation>
      <xs:documentation>(usage restrictions) – Container for information about the conditions that affect use of a bibliographic item after access has been granted.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mei:model.textPhraseLike.limited"/>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.bibl.attributes"/>
      <xs:attributeGroup ref="mei:att.lang.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="watermark">
