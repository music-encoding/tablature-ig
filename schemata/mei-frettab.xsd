  </xs:element>
  <xs:element name="mRpt2">
    <xs:annotation>
      <xs:documentation>(2-measure repeat) – An indication that the previous two measures should be repeated.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt2.log.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt2.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt2.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="mSpace">
    <xs:annotation>
      <xs:documentation>(measure space) – A measure containing only empty space in any meter.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.mSpace.log.attributes"/>
      <xs:attributeGroup ref="mei:att.mSpace.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.mSpace.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.mSpace.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="multiRest">
    <xs:annotation>
      <xs:documentation>(multiple rest) – Multiple measures of rest compressed into a single symbol, frequently found in performer parts.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRest.log.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRest.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRest.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRest.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="multiRpt">
    <xs:annotation>
      <xs:documentation>(multiple repeat) – Multiple repeated measures.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRpt.log.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRpt.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.multiRpt.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="octave">
    <xs:annotation>
      <xs:documentation>An indication that a passage should be performed one or more octaves above or below its written pitch.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="mei:model.textPhraseLike.limited"/>
