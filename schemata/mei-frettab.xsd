      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:length value="0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.mmTempo.attribute.mm.dots">
    <xs:attribute name="mm.dots" type="mei:data.AUGMENTDOT">
      <xs:annotation>
        <xs:documentation>Records the number of augmentation dots required by a dotted metronome unit.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
