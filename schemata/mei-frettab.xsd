      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt.log.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.mRpt.anl.attributes"/>
